        </sequence>
      </extension>
    </complexContent>
  </complexType>

  <complexType name="inputStream" abstract="true">
    <sequence/>
  </complexType>

  <complexType name="ruolo">
    <sequence>
      <element name="desc" type="string" tns:minOccurs="0"/>
      <element name="idRuolo" type="int" tns:minOccurs="0"/>
