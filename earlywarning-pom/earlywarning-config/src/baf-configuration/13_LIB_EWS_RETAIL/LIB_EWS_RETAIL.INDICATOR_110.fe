VAR formulaName = "[LIB_EWS_RETAIL.INDICATOR_110.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
 
CAST(
	CASE
		WHEN ULT_RT_SCD_RIDMAV_DT IS NULL THEN NULL
		ELSE (ULT_RT_SCD_RIDMAV_DT - CURRENT_DATE)
	END AS DECIMAL(10,0)) AS IND_110,
	
	CASE
		WHEN ULT_RT_SCD_RIDMAV_DT IS NULL THEN 'RTI000_1'
		ELSE ''
	END AS ERROR_MSG_IND_110
";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;