VAR formulaName = "[LIB_EWS_RETAIL.INDICATOR_114.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
 
CAST(
	CASE
		WHEN IMP_SCONFINO_CC IS NULL OR IMP_SCONFINO_CC < 0 THEN NULL
		ELSE IMP_SCONFINO_CC
	END AS DECIMAL(18,6)) AS IND_114,
	
	CASE
		WHEN IMP_SCONFINO_CC IS NULL THEN 'SCC000_1'
		WHEN IMP_SCONFINO_CC < 0 THEN 'SCC000_2'
		ELSE ''
	END AS ERROR_MSG_IND_114
";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;