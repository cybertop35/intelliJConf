VAR formulaName = "[LIB_EWS_RETAIL.INDICATOR_55.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

CASE
	WHEN (NUM_GIO_SCONFINO > 30) AND abs(IMP_SCONFINO) > 100 AND (GREATEST((abs(IMP_SCONFINO/IMP_UTILIZZO_TOT)),(abs(IMP_SCONF_CUM_90GG/IMP_UTILIZZO_CUM_90GG))))> 0.02 THEN '1'
	ELSE '0'
end AS IND_55,

CASE
	when IND_55 IS NULL THEN 'XRA000_1'
	else ''
END AS ERROR_MSG_IND_55
		";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
