VAR formulaName = "[LIB_EWS_RETAIL.INDICATOR_209.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

CAST(
	CASE
		WHEN (IMP_SCONFINO IS NULL OR IMP_UTILIZZO_TOT IS NULL) THEN NULL
		ELSE CAST ((IMP_SCONFINO / IMP_UTILIZZO_TOT) AS DECIMAL(30,4))
	END AS DECIMAL(18,6)) AS IND_209,

CASE
	WHEN NUM_GIO_SCONFINO IS NULL THEN 'XRA000_1'
	ELSE '' 
END AS ERROR_MSG_IND_209

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;