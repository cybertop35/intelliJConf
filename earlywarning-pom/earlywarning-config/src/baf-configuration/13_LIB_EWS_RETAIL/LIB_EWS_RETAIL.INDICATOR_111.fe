VAR formulaName = "[LIB_EWS_RETAIL.INDICATOR_111.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
 
CAST(
	CASE
		WHEN IMP_ARRETRATO_RIDMAV IS NULL OR IMP_ARRETRATO_RIDMAV < 0 THEN NULL
		ELSE IMP_ARRETRATO_RIDMAV
	END AS DECIMAL(18,6)) AS IND_111,
	
	CASE
		WHEN IMP_ARRETRATO_RIDMAV IS NULL THEN 'RTI000_1'
		WHEN IMP_ARRETRATO_RIDMAV < 0 THEN 'RTI000_2'
		ELSE ''
	END AS ERROR_MSG_IND_111
";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;