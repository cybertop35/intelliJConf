VAR formulaName = "[LIB_EWS_RETAIL.INDICATOR_116.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
 
CAST(
	CASE
		WHEN FLG_PERFNOPE IS NULL OR FLG_PERFNOPE < 0  THEN '0'
		ELSE FLG_PERFNOPE
	END AS CHAR(1)) AS IND_116,
	
	CASE
		WHEN FLG_PERFNOPE IS NULL THEN 'FORB000_1'
		WHEN FLG_PERFNOPE < 0 THEN 'FORB000_2'
		ELSE ''
	END AS ERROR_MSG_IND_116
";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;