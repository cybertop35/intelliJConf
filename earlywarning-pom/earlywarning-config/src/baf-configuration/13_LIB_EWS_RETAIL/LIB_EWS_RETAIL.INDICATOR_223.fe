				(	CAST((
						(	
							zeroifnull(IMP_UTIL_PROD_SCAD_M0) + 
							zeroifnull(IMP_UTIL_PROD_SCAD_M1) + 
							zeroifnull(IMP_UTIL_PROD_SCAD_M2)
