		 WHEN NUM_NUM_IND_223 > 0 AND DEN_NUM_IND_223 = 0 THEN 1000000
		 WHEN NUM_NUM_IND_223 = 0 AND DEN_NUM_IND_223 = 0 THEN 0
		 WHEN NUM_NUM_IND_223 < 0 OR DEN_NUM_IND_223 < 0 THEN NULL
