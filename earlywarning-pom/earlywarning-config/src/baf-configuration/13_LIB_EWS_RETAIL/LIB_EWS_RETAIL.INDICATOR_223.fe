							NVL2(IMP_UTIL_PROD_SCAD_M0,1,0) +
							NVL2(IMP_UTIL_PROD_SCAD_M1,1,0) +
							NVL2(IMP_UTIL_PROD_SCAD_M2,1,0)
						)
					) AS DECIMAL(30,6)))
			 ELSE NULL
	END AS NUM_DEN_IND_223,

	CASE WHEN IMP_ACC_PROD_SCAD_M0 IS NOT NULL OR
			 IMP_ACC_PROD_SCAD_M1 IS NOT NULL OR
			 IMP_ACC_PROD_SCAD_M2 IS NOT NULL THEN 
				(	CAST((
						(	
							zeroifnull(IMP_ACC_PROD_SCAD_M0) + 
							zeroifnull(IMP_ACC_PROD_SCAD_M1) + 
							zeroifnull(IMP_ACC_PROD_SCAD_M2)
						)
							/
						(
							NVL2(IMP_ACC_PROD_SCAD_M0,1,0) +
							NVL2(IMP_ACC_PROD_SCAD_M1,1,0) +
							NVL2(IMP_ACC_PROD_SCAD_M2,1,0)
						)
					) AS DECIMAL(30,6)))
			 ELSE NULL
	END AS DEN_DEN_IND_223,

	CASE 
		WHEN NUM_DEN_IND_223 IS NULL OR DEN_DEN_IND_223 IS NULL THEN NULL
		WHEN NUM_DEN_IND_223 > 0 AND DEN_DEN_IND_223 = 0 THEN 1000000
		WHEN NUM_DEN_IND_223 = 0 AND DEN_DEN_IND_223 = 0 THEN 0
		WHEN NUM_DEN_IND_223 < 0 OR DEN_DEN_IND_223 < 0 THEN NULL
		ELSE CAST((NUM_DEN_IND_223 / DEN_DEN_IND_223 ) AS DECIMAL(18,6))
	END AS DEN_IND_223,
	
CAST(	
	CASE
		WHEN NUM_IND_223 IS NULL AND DEN_IND_223 IS NULL    THEN NULL
		WHEN NUM_IND_223 = 0 AND DEN_IND_223 = 0            THEN 0
		WHEN NUM_IND_223 IS NULL AND DEN_IND_223 > 0        THEN NULL
		WHEN NUM_IND_223 IS NULL AND DEN_IND_223 = 0        THEN NULL
		WHEN NUM_IND_223 > 0 AND DEN_IND_223 IS NULL        THEN NULL
		WHEN NUM_IND_223 = 0 AND DEN_IND_223 IS NULL        THEN NULL
		WHEN NUM_IND_223 < 0 AND DEN_IND_223 > 0            THEN NULL
		WHEN NUM_IND_223 < 0 AND DEN_IND_223 = 0            THEN NULL
		WHEN NUM_IND_223 < 0 AND DEN_IND_223 < 0            THEN NULL
		WHEN NUM_IND_223 > 0 AND DEN_IND_223 < 0            THEN NULL
		WHEN NUM_IND_223 = 0 AND DEN_IND_223 < 0            THEN NULL
		WHEN NUM_IND_223 > 0 AND DEN_IND_223 = 0            THEN NULL
		ELSE (NUM_IND_223 /DEN_IND_223) 
	END AS DECIMAL(18,6)) AS IND_223 ,
				
	CASE	
		WHEN NUM_NUM_IND_223 > 0 AND DEN_NUM_IND_223 = 0 	   		THEN 'CRZER002_11;'     
		WHEN NUM_NUM_IND_223 = 0 AND DEN_NUM_IND_223 = 0        		THEN 'CRZER002_12;'    
		ELSE ''
	END  ||
	CASE	
		WHEN NUM_DEN_IND_223 > 0 AND DEN_DEN_IND_223 = 0 	   		THEN 'CRZER002_21;'    
		WHEN NUM_DEN_IND_223 = 0 AND DEN_DEN_IND_223 = 0        		THEN 'CRZER002_22;'    
		ELSE ''
	END	 || 
	CASE 
		WHEN NUM_IND_223 IS NULL AND DEN_IND_223 IS NULL    THEN 'CRZER000_1'    
		WHEN NUM_IND_223 = 0 AND DEN_IND_223 = 0            THEN 'CRZER002_1'
		WHEN NUM_IND_223 IS NULL AND DEN_IND_223 > 0        THEN 'CRZER003_1'
		WHEN NUM_IND_223 IS NULL AND DEN_IND_223 = 0        THEN 'CRZER003_1'
		WHEN NUM_IND_223 > 0 AND DEN_IND_223 IS NULL        THEN 'CRZER003_2'
		WHEN NUM_IND_223 = 0 AND DEN_IND_223 IS NULL        THEN 'CRZER003_2'
		WHEN NUM_IND_223 < 0 AND DEN_IND_223 > 0            THEN 'CRZER004_1'
		WHEN NUM_IND_223 < 0 AND DEN_IND_223 = 0            THEN 'CRZER004_1'
		WHEN NUM_IND_223 < 0 AND DEN_IND_223 < 0            THEN 'CRZER004_1; CRZER004_2'
		WHEN NUM_IND_223 > 0 AND DEN_IND_223 < 0            THEN 'CRZER004_2'
		WHEN NUM_IND_223 = 0 AND DEN_IND_223 < 0            THEN 'CRZER004_2'
		WHEN NUM_IND_223 > 0 AND DEN_IND_223 = 0            THEN 'CRZER004_3'
		ELSE ''
		
	END AS ERROR_MSG_IND_223

	";

	DEBUGMESSAGE(formulaName, sql);
	DEBUGMESSAGE(formulaName, "END");

	return sql;

