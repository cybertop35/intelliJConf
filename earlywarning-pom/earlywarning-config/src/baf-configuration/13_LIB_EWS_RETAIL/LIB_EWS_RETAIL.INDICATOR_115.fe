VAR formulaName = "[LIB_EWS_RETAIL.INDICATOR_115.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
 
CAST(
	CASE
		WHEN NUM_GG_SCONFINO IS NULL OR NUM_GG_SCONFINO < 0 THEN NULL
		ELSE NUM_GG_SCONFINO
	END AS DECIMAL(10,0)) AS IND_115,
	
	CASE
		WHEN NUM_GG_SCONFINO IS NULL THEN 'SCC000_1'
		WHEN NUM_GG_SCONFINO < 0 THEN 'SCC000_2'
		ELSE ''
	END AS ERROR_MSG_IND_115
";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;