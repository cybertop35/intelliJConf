VAR formulaName = "[LIB_EWS_BE.INDICATOR_80.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2) AS DENOM_ID80,
(CASE 
	WHEN DENOM_ID80 = 0 THEN NULL
	ELSE (SALES_Y0/((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2)) 
END) AS IND_80";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
