VAR formulaName = "[LIB_EWS_BE.INDICATOR_71.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "(CASE
WHEN COST_SOLD_Y1 = 0 AND COST_SOLD_Y0 > 0 THEN NULL
WHEN COST_SOLD_Y1 = 0 AND COST_SOLD_Y0 < 0 THEN NULL
WHEN COST_SOLD_Y1 = 0 AND COST_SOLD_Y0 = 0 THEN NULL  
ELSE ((COST_SOLD_Y0/COST_SOLD_Y1)-1)
END) AS IND_71";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
