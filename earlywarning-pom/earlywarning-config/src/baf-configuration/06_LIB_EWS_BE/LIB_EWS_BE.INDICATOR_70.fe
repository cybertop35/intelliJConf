VAR formulaName = "[LIB_EWS_BE.INDICATOR_70.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "CASE 
	WHEN WORKING_CAPITAL_Y1 = 0 AND  WORKING_CAPITAL_Y0 > 0 THEN NULL
	WHEN WORKING_CAPITAL_Y1 = 0 AND  WORKING_CAPITAL_Y0 < 0 THEN NULL
	WHEN WORKING_CAPITAL_Y1 = 0 AND  WORKING_CAPITAL_Y0 = 0 THEN NULL
	ELSE ((WORKING_CAPITAL_Y0/WORKING_CAPITAL_Y1 ) -1)
END AS IND_70";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
