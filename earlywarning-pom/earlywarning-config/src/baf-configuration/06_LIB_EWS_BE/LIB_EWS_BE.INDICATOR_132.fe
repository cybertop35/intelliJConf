VAR formulaName = "[LIB_EWS_BE.INDICATOR_132.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

CASE 
	WHEN REGULAMENTARY_SEGMENT = 'Corporate' THEN 0.03870615
	WHEN REGULAMENTARY_SEGMENT = 'Retail' THEN 0.02951718
	ELSE NULL
END AS MISSING_VALUE_IND_132,

CASE
	WHEN (SALES_Y1 <> 0 AND SALES_Y1 IS NOT NULL AND TOTAL_DEBT_Y1 IS NOT NULL) THEN (TOTAL_DEBT_Y1 / (SALES_Y1))
