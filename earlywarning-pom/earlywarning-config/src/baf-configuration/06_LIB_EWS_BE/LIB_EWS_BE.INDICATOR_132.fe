CASE
	WHEN (SALES_Y0 <> 0 AND SALES_Y0 IS NOT NULL AND TOTAL_DEBT_Y0 IS NOT NULL) THEN (TOTAL_DEBT_Y0 / (SALES_Y0))
	WHEN (TOTAL_DEBT_Y0 IS NULL) THEN NULL
	WHEN (SALES_Y0 = 0) THEN NULL
	WHEN (SALES_Y0 IS NULL) THEN NULL
END AS NUM_IND_132, 

CASE
	WHEN (DEN_IND_132 <> 0 AND DEN_IND_132 IS NOT NULL AND NUM_IND_132 IS NOT NULL) THEN (NUM_IND_132 / DEN_IND_132)-1
	WHEN (NUM_IND_132 IS NULL) THEN MISSING_VALUE_IND_132
	WHEN (DEN_IND_132 = 0) THEN MISSING_VALUE_IND_132
	WHEN (DEN_IND_132 IS NULL) THEN MISSING_VALUE_IND_132 
END AS IND_132,

CASE 
	WHEN (DEN_IND_132 <> 0 AND DEN_IND_132 IS NOT NULL AND NUM_IND_132 IS NOT NULL) THEN ''
	WHEN (NUM_IND_132 IS NULL) THEN 'E0001 '
	WHEN (DEN_IND_132 = 0) THEN 'E0001 '
	WHEN (DEN_IND_132 IS NULL) THEN 'E0001 ' 
END AS ERROR_MSG_IND_132



";


