END || CASE
WHEN (EQUITY_Y1 < 0 AND EQUITY_Y0 IS NULL) THEN 'ERROR; ' else ''
END || CASE
WHEN (EQUITY_Y1 IS NULL AND EQUITY_Y0 >0) THEN 'ERROR; ' else ''
END || CASE   
WHEN (EQUITY_Y1 IS NULL AND EQUITY_Y0 <0) THEN 'ERROR; ' else ''
END || CASE
WHEN (EQUITY_Y1 IS NULL AND EQUITY_Y0 =0) THEN 'ERROR; ' else ''
END || CASE
WHEN (EQUITY_Y1 IS NULL AND EQUITY_Y0 IS NULL) THEN 'ERROR; ' else ''
END
) AS ERROR_MSG_IND_35";


