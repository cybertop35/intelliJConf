VAR formulaName = "[LIB_EWS_BE.INDICATOR_98.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(CASE
                WHEN ((TOTAL_ASSETS_Y1-TOTAL_LIABILITIES_Y1-INTANGIBLE_ASSETS_Y1) <> 0 AND (TOTAL_ASSETS_Y1-TOTAL_LIABILITIES_Y1-INTANGIBLE_ASSETS_Y1) IS NOT NULL AND (TOTAL_ASSETS_Y0-TOTAL_LIABILITIES_Y0-INTANGIBLE_ASSETS_Y0) IS NOT NULL) 
                THEN (
                (CAST (TOTAL_ASSETS_Y0 AS DECIMAL(18,6)) - CAST (TOTAL_LIABILITIES_Y0 AS DECIMAL(18,6)) - CAST (INTANGIBLE_ASSETS_Y0 AS DECIMAL(18,6)))

                / 

                (CAST (TOTAL_ASSETS_Y1 AS DECIMAL(18,6)) - CAST (TOTAL_LIABILITIES_Y1 AS DECIMAL(18,6)) - CAST (INTANGIBLE_ASSETS_Y1 AS DECIMAL(18,6))))-1

                WHEN ((TOTAL_ASSETS_Y0-TOTAL_LIABILITIES_Y0-INTANGIBLE_ASSETS_Y0) IS NULL) THEN MISSING_VAL_IND_98
                WHEN ((TOTAL_ASSETS_Y1-TOTAL_LIABILITIES_Y1-INTANGIBLE_ASSETS_Y1) = 0) THEN MISSING_VAL_IND_98
                WHEN ((TOTAL_ASSETS_Y1-TOTAL_LIABILITIES_Y1-INTANGIBLE_ASSETS_Y1) IS NULL) THEN MISSING_VAL_IND_98 
END AS DECIMAL(18,6)) AS IND_98_TMP,

CAST(CASE
		WHEN (VAL_MAX_IND_98 IS NOT NULL AND IND_98_TMP > VAL_MAX_IND_98) THEN VAL_MAX_IND_98
		WHEN (VAL_MIN_IND_98 IS NOT NULL AND IND_98_TMP < VAL_MIN_IND_98) THEN VAL_MIN_IND_98
		ELSE IND_98_TMP
END AS DECIMAL(18,6)) AS IND_98,

CASE 
                WHEN ((TOTAL_ASSETS_Y1-TOTAL_LIABILITIES_Y1-INTANGIBLE_ASSETS_Y1) <> 0 AND (TOTAL_ASSETS_Y1-TOTAL_LIABILITIES_Y1-INTANGIBLE_ASSETS_Y1) IS NOT NULL AND (TOTAL_ASSETS_Y0-TOTAL_LIABILITIES_Y0-INTANGIBLE_ASSETS_Y0) IS NOT NULL) THEN ''
                WHEN ((TOTAL_ASSETS_Y0-TOTAL_LIABILITIES_Y0-INTANGIBLE_ASSETS_Y0) IS NULL) THEN 'E0001'
                WHEN ((TOTAL_ASSETS_Y1-TOTAL_LIABILITIES_Y1-INTANGIBLE_ASSETS_Y1) = 0) THEN 'E0001'
                WHEN ((TOTAL_ASSETS_Y1-TOTAL_LIABILITIES_Y1-INTANGIBLE_ASSETS_Y1) IS NULL) THEN 'E0001' 
END ||
CASE
     WHEN (VAL_MAX_IND_98 IS NOT NULL AND IND_98_TMP > VAL_MAX_IND_98) THEN 'E0002'
	 WHEN (VAL_MIN_IND_98 IS NOT NULL AND IND_98_TMP < VAL_MIN_IND_98) THEN 'E0002'
	ELSE ''
END AS ERROR_MSG_IND_98

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
