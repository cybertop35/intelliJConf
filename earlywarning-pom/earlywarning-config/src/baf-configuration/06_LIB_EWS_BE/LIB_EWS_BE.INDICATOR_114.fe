VAR formulaName = "[LIB_EWS_BE.INDICATOR_114.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "CASE 
WHEN NET_INCOME_Y1 = 0 AND NET_INCOME_Y0 > 0 THEN NULL
WHEN NET_INCOME_Y1 = 0 AND NET_INCOME_Y0 < 0 THEN NULL
WHEN NET_INCOME_Y1 = 0 AND NET_INCOME_Y0 = 0 THEN NULL
ELSE(NET_INCOME_Y0/NET_INCOME_Y1)-1
END AS IND_114";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
