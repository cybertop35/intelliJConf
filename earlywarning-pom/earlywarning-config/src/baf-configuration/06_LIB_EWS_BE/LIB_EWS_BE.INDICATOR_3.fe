VAR formulaName = "[LIB_EWS_BE.INDICATOR_3.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

CAST
	 (CASE 
		WHEN NULLIF(FLG_BLOCKED_ACCOUNT,'') IS NULL THEN CAST(0 AS CHAR(1))
		ELSE FLG_BLOCKED_ACCOUNT 
	END AS CHAR(1)) AS IND_3,

	CASE
		WHEN NULLIF(FLG_BLOCKED_ACCOUNT,'') IS NULL THEN 'E0001 ;' ELSE ''
	END ERROR_MSG_IND_3

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
