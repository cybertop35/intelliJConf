VAR formulaName = "[LIB_EWS_BE.INDICATOR_78.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "CASE
                WHEN (TOTAL_ASSETS_Y0 = 0 AND ST_LIABILITIES_Y0 >0) THEN NULL
                WHEN (TOTAL_ASSETS_Y0 = 0 AND ST_LIABILITIES_Y0 <0) THEN NULL
                WHEN (TOTAL_ASSETS_Y0 = 0 AND ST_LIABILITIES_Y0 =0) THEN NULL
		ELSE (ST_LIABILITIES_Y0 / TOTAL_ASSETS_Y0)
END AS IND_78";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
