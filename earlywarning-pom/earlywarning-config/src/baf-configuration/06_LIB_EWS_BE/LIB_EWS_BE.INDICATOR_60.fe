VAR formulaName = "[LIB_EWS_BE.INDICATOR_60.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(CASE
	   WHEN 
	   		INFLOWS_ACCOUNT_M0  IS NULL AND 
	   		INFLOWS_ACCOUNT_M1  IS NULL AND 
	   		INFLOWS_ACCOUNT_M2  IS NULL AND
