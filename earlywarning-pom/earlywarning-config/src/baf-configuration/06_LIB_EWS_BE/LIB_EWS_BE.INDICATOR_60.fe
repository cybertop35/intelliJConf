		INFLOWS_ACCOUNT_M9  IS NULL AND 
		INFLOWS_ACCOUNT_M10 IS NULL AND 
		INFLOWS_ACCOUNT_M11 IS NULL THEN  'E0001' 
		ELSE ''
END || 
CASE
		WHEN (VAL_MAX_IND_60 IS NOT NULL AND IND_60_TMP > VAL_MAX_IND_60) OR (VAL_MIN_IND_60 IS NOT NULL AND IND_60_TMP < VAL_MIN_IND_60) THEN 'E0002'
		ELSE ''
END

) AS ERROR_MSG_IND_60
";


