VAR formulaName = "[LIB_EWS_BE.INDICATOR_40.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "(CASE 
WHEN TOTAL_COLLATERAL = 0 AND APPROVED_LOANS > 0 THEN NULL
WHEN TOTAL_COLLATERAL = 0 AND APPROVED_LOANS < 0 THEN NULL
WHEN TOTAL_COLLATERAL = 0 AND APPROVED_LOANS = 0 THEN NULL
ELSE ((APPROVED_LOANS/TOTAL_COLLATERAL)-1)
END) AS IND_40";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
