	WHEN (INTEREST_EXPENSES_Y0 IS NULL AND EBITDA_Y0 <0) THEN 0
	WHEN (INTEREST_EXPENSES_Y0 IS NULL AND EBITDA_Y0 =0) THEN 0
	WHEN (INTEREST_EXPENSES_Y0 IS NULL AND EBITDA_Y0 IS NULL) THEN 0
