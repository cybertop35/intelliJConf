VAR formulaName = "[LIB_EWS_BE.INDICATOR_100.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "(CASE 
	WHEN INTEREST_EXPENSES_Y0 = 0 THEN NULL
	WHEN INTEREST_EXPENSES_Y1 = 0 THEN NULL 
	WHEN ((EBITDA_Y1/INTEREST_EXPENSES_Y1)-1) = 0 THEN NULL
	ELSE (((EBITDA_Y0/INTEREST_EXPENSES_Y0)/(EBITDA_Y1/INTEREST_EXPENSES_Y1)-1))
END) AS IND_100";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
