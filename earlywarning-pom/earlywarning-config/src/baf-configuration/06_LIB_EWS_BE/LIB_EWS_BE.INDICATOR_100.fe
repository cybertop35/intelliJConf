CASE
	WHEN (INTEREST_EXPENSES_Y0 > 0 AND EBITDA_Y0 >0) THEN (EBITDA_Y0 / (INTEREST_EXPENSES_Y0))
	WHEN (INTEREST_EXPENSES_Y0 > 0 AND EBITDA_Y0 =0) THEN (EBITDA_Y0 / (INTEREST_EXPENSES_Y0))
