	WHEN (INTEREST_EXPENSES_Y1 = 0 AND EBITDA_Y1 <0) THEN 0
	WHEN (INTEREST_EXPENSES_Y1 = 0 AND EBITDA_Y1 =0) THEN 0
	WHEN (INTEREST_EXPENSES_Y1 = 0 AND EBITDA_Y1 IS NULL) THEN 0

	WHEN (INTEREST_EXPENSES_Y1 < 0 AND EBITDA_Y1 >0) THEN (EBITDA_Y1 / (INTEREST_EXPENSES_Y1))         
