	WHEN (DEN_IND_100 = 0 AND NUM_IND_100 <0) THEN MISSING_VALUE_IND_100
	WHEN (DEN_IND_100 = 0 AND NUM_IND_100 =0) THEN MISSING_VALUE_IND_100
