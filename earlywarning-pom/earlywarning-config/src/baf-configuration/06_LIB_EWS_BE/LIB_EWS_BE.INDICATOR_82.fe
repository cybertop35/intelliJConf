	WHEN (EBIT_Y1 < 0 AND EBIT_Y0 IS NULL) THEN 0

	WHEN (EBIT_Y1 IS NULL AND EBIT_Y0 >0) THEN 0
	WHEN (EBIT_Y1 IS NULL AND EBIT_Y0 <0) THEN 0
	WHEN (EBIT_Y1 IS NULL AND EBIT_Y0 =0) THEN 0
	WHEN (EBIT_Y1 IS NULL AND EBIT_Y0 IS NULL) THEN 0

