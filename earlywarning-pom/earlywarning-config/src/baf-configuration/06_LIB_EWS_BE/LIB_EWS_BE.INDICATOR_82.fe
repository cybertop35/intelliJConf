sql = "
CASE 
	WHEN (EBIT_Y1 > 0 AND EBIT_Y0 >0) THEN (EBIT_Y0/EBIT_Y1)
	WHEN (EBIT_Y1 > 0 AND EBIT_Y0 =0) THEN (EBIT_Y0/EBIT_Y1)
	WHEN (EBIT_Y1 > 0 AND EBIT_Y0 <0) THEN (EBIT_Y0/EBIT_Y1)
	WHEN (EBIT_Y1 > 0 AND EBIT_Y0 IS NULL) THEN 0

