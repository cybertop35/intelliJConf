VAR formulaName = "[LIB_EWS_BE.INDICATOR_5.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

-999 AS MISSING_VALUE_IND_5,

CAST(CASE
	WHEN EXTERNAL_DAYS_PAST_DUE IS NULL THEN MISSING_VALUE_IND_5
	ELSE (EXTERNAL_DAYS_PAST_DUE) 
END AS DECIMAL(10,0)) AS IND_5,

(CASE
	WHEN EXTERNAL_DAYS_PAST_DUE IS NULL THEN 'E0001 ' ELSE ''
END
) AS ERROR_MSG_IND_5

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
