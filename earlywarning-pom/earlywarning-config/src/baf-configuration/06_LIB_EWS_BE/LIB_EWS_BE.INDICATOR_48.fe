VAR sql = "";

sql = "CASE
        WHEN ((ST_BANK_DEBT_Y0+ST_FIN_DEBT_Y0+(LT_FINANCIAL_DEBT/3.5)-CASH_AMOUNT_Y0+INTEREST_EXPENSES_Y0) = 0 AND (NET_INCOME_Y0+AMORTIZATION_Y0+INTEREST_EXPENSES_Y0) >0) THEN NULL         
	WHEN ((ST_BANK_DEBT_Y0+ST_FIN_DEBT_Y0+(LT_FINANCIAL_DEBT/3.5)-CASH_AMOUNT_Y0+INTEREST_EXPENSES_Y0) = 0 AND (NET_INCOME_Y0+AMORTIZATION_Y0+INTEREST_EXPENSES_Y0) <0) THEN NULL
	WHEN ((ST_BANK_DEBT_Y0+ST_FIN_DEBT_Y0+(LT_FINANCIAL_DEBT/3.5)-CASH_AMOUNT_Y0+INTEREST_EXPENSES_Y0) = 0 AND (NET_INCOME_Y0+AMORTIZATION_Y0+INTEREST_EXPENSES_Y0) =0) THEN NULL
	ELSE ((NET_INCOME_Y0+AMORTIZATION_Y0+INTEREST_EXPENSES_Y0)/(ST_BANK_DEBT_Y0+ST_FIN_DEBT_Y0+(LT_FINANCIAL_DEBT/3.5)-CASH_AMOUNT_Y0+INTEREST_EXPENSES_Y0))
  
END AS IND_48";


