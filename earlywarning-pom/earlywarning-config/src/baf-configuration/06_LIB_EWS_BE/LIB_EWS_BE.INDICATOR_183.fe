VAR formulaName = "[LIB_EWS_BE.INDICATOR_183.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "(INFLOWS_ACCOUNT_M0+OUTFLOWS_AMOUNT_M0) AS NUM_IND_183,
CASE WHEN OVERDRAFT_LIMIT_M0=0 AND NUM_IND_183=0 THEN NULL
WHEN OVERDRAFT_LIMIT_M0=0 AND NUM_IND_183<0 THEN NULL
WHEN OVERDRAFT_LIMIT_M0=0 AND NUM_IND_183>0 THEN NULL
ELSE NUM_IND_183/OVERDRAFT_LIMIT_M0
END AS IND_183";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;

