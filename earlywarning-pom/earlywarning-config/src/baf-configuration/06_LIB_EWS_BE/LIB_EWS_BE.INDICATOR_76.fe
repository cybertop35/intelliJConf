VAR formulaName = "[LIB_EWS_BE.INDICATOR_76.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "(ST_ASSETS_Y0 / ST_LIABILITIES_Y0) as NUM_ID76,
(ST_ASSETS_Y1 / ST_LIABILITIES_Y1) as DENOM_ID76,
(CASE 
	WHEN ST_LIABILITIES_Y0 = 0 THEN NULL
	WHEN ST_LIABILITIES_Y1 = 0 THEN NULL
	WHEN DENOM_ID76 = 0 THEN NULL
	ELSE (NUM_ID76 / DENOM_ID76) 
END) AS ID76";


DEBUGMESSAGE(formulaName, sql);
