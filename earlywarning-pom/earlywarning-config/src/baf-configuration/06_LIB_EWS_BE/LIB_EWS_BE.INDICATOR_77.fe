VAR formulaName = "[LIB_EWS_BE.INDICATOR_77.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "(CASE 
WHEN ST_LIABILITIES_Y1 = 0 AND ST_LIABILITIES_Y0 > 0 THEN NULL
WHEN ST_LIABILITIES_Y1 = 0 AND ST_LIABILITIES_Y0 < 0 THEN NULL
WHEN ST_LIABILITIES_Y1 = 0 AND ST_LIABILITIES_Y0 = 0 THEN NULL
ELSE ((ST_LIABILITIES_Y0/ST_LIABILITIES_Y1)-1)
END) AS IND_77";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
