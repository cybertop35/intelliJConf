VAR formulaName = "[LIB_EWS_BE.INDICATOR_95.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "(SALES_Y0 - COST_SOLD_Y0) AS NUM_IND_95,
(SALES_Y1 - COST_SOLD_Y1) AS DEN_IND_95,
CASE WHEN DEN_IND_95=0 AND NUM_IND_95=0 THEN NULL
WHEN DEN_IND_95=0 AND NUM_IND_95>0 THEN NULL
WHEN DEN_IND_95=0 AND NUM_IND_95<0 THEN NULL
ELSE ((NUM_IND_95/DEN_IND_95)-1)
END AS IND_95";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
