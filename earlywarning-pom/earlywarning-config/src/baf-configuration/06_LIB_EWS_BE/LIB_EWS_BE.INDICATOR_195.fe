VAR formulaName = "[LIB_EWS_BE.INDICATOR_195.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

-999 AS MISSING_VALUE_IND_195,

CASE 
	WHEN UNPAID_INSTAL_LEASING IS NULL THEN MISSING_VALUE_IND_195
	ELSE CAST(UNPAID_INSTAL_LEASING AS DECIMAL(18,6))
END AS IND_195,

CASE
	WHEN UNPAID_INSTAL_LEASING IS NULL THEN 'E0001 ' ELSE ''
END ERROR_MSG_IND_195


";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
