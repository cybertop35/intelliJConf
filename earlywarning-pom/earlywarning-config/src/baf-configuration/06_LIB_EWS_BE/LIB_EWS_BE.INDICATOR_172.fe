VAR formulaName = "[LIB_EWS_BE.INDICATOR_172.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "CAST(
CASE 
WHEN INFLOWS_ACCOUNT_M0 IS NULL THEN 0
WHEN INFLOWS_ACCOUNT_M1 IS NULL THEN 0
WHEN INFLOWS_ACCOUNT_M0<INFLOWS_ACCOUNT_M1 THEN 1 ELSE 0
END AS CHAR(1)) AS IND_172,

CASE 
WHEN INFLOWS_ACCOUNT_M0 IS NULL
OR 
INFLOWS_ACCOUNT_M1 IS NULL
THEN 'E0001 ' ELSE ''
END AS ERROR_MSG_IND_172";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
