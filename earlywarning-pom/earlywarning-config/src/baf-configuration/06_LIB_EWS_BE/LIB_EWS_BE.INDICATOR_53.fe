VAR formulaName = "[LIB_EWS_BE.INDICATOR_53.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

0 AS MISSING_VALUE_IND_53,

CAST
	 (CASE 
		WHEN NULLIF(FLG_EXPOSURE,'') IS NULL THEN MISSING_VALUE_IND_53
		ELSE 1 
		END
	 AS CHAR(1)
	 )
