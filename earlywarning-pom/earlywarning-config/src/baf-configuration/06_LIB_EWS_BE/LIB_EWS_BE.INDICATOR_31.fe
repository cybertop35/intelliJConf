VAR formulaName = "[LIB_EWS_BE.INDICATOR_31.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

CAST(CASE 
		WHEN COLLATERAL_EXPIRING_DATE IS NULL OR REPORT_DATE IS NULL THEN 0
		WHEN COLLATERAL_EXPIRING_DATE-REPORT_DATE <=90 THEN 1 
		ELSE 0
	END AS CHAR(1)) AS IND_31,
	
CASE
 	WHEN COLLATERAL_EXPIRING_DATE IS NULL OR REPORT_DATE IS NULL THEN 'E0001' ELSE ''
END AS ERROR_MSG_IND_31

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
