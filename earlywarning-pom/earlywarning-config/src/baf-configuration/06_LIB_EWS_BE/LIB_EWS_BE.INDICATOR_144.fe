VAR formulaName = "[LIB_EWS_BE.INDICATOR_144.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

-999 AS MISSING_VALUE_IND_144,

CAST(
CASE
                WHEN ((SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2) <> 0 AND (SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2) IS NOT NULL AND (SUM_INVOICES_UNPAID_M0+SUM_INVOICES_UNPAID_M1+SUM_INVOICES_UNPAID_M2) + (SUM_CALLED_BACK_INVOICES_M0+SUM_CALLED_BACK_INVOICES_M1+SUM_CALLED_BACK_INVOICES_M2) IS NOT NULL) THEN CAST((CAST((CAST((SUM_INVOICES_UNPAID_M0+SUM_INVOICES_UNPAID_M1+SUM_INVOICES_UNPAID_M2)AS DECIMAL(18,6)) + CAST((SUM_CALLED_BACK_INVOICES_M0+SUM_CALLED_BACK_INVOICES_M1+SUM_CALLED_BACK_INVOICES_M2)AS DECIMAL(18,6)))AS DECIMAL(18,6)) / CAST((SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2)AS DECIMAL(18,6)))AS DECIMAL(18,6))
                WHEN ((SUM_INVOICES_UNPAID_M0+SUM_INVOICES_UNPAID_M1+SUM_INVOICES_UNPAID_M2) + (SUM_CALLED_BACK_INVOICES_M0+SUM_CALLED_BACK_INVOICES_M1+SUM_CALLED_BACK_INVOICES_M2) IS NULL) THEN MISSING_VALUE_IND_144
                WHEN ((SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2) = 0) THEN MISSING_VALUE_IND_144
                WHEN ((SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2) IS NULL) THEN MISSING_VALUE_IND_144 
END  AS DECIMAL(18,6)) AS IND_144,

CASE 
                WHEN ((SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2) <> 0 AND (SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2) IS NOT NULL AND (SUM_INVOICES_UNPAID_M0+SUM_INVOICES_UNPAID_M1+SUM_INVOICES_UNPAID_M2) + (SUM_CALLED_BACK_INVOICES_M0+SUM_CALLED_BACK_INVOICES_M1+SUM_CALLED_BACK_INVOICES_M2) IS NOT NULL) THEN ''
                WHEN ((SUM_INVOICES_UNPAID_M0+SUM_INVOICES_UNPAID_M1+SUM_INVOICES_UNPAID_M2) + (SUM_CALLED_BACK_INVOICES_M0+SUM_CALLED_BACK_INVOICES_M1+SUM_CALLED_BACK_INVOICES_M2) IS NULL) THEN 'E0001 '
                WHEN ((SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2) = 0) THEN 'E0001 '
                WHEN ((SUM_INVOICES_DUE_M0+SUM_INVOICES_DUE_M1+SUM_INVOICES_DUE_M2) IS NULL) THEN 'E0001 ' 
END AS ERROR_MSG_IND_144


";

