VAR formulaName = "[LIB_EWS_BE.INDICATOR_1.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
CAST 
	(CASE WHEN DAYS_PAST_DUE IS NULL OR DAYS_PAST_DUE < 0 THEN MISSING_VAL_IND_1
		ELSE DAYS_PAST_DUE
	END AS DECIMAL(10,0)) AS IND_1,

	 CASE WHEN DAYS_PAST_DUE IS NULL OR DAYS_PAST_DUE < 0 THEN 'E0001' ELSE ''
	END AS ERROR_MSG_IND_1
";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
