VAR formulaName = "[LIB_EWS_BE.INDICATOR_2.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "CASE
           WHEN DAYS_PAST_DUE IS NULL THEN 0
           WHEN DAYS_PAST_DUE > 90 THEN 1
           ELSE 0

END AS IND_2";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
