VAR formulaName = "[LIB_EWS_BE.INDICATOR_11.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "(CASE 
WHEN(MONITORING_RATING_M0-MONITORING_RATING_M1)<0 THEN 1 ELSE 0
END) +
(CASE 
WHEN(MONITORING_RATING_M1-MONITORING_RATING_M2)<0 THEN 1 ELSE 0
END) +
(CASE 
WHEN(MONITORING_RATING_M2-MONITORING_RATING_M3)<0 THEN 1 ELSE 0
END) AS IND_11";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
