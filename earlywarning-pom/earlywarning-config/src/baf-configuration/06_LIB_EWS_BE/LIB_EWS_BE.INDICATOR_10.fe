VAR formulaName = "[LIB_EWS_BE.INDICATOR_10.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST
	(CASE 
		WHEN NULLIF(FLG_STATUTORY_CHANGE, '') IS NULL THEN '0'
		ELSE FLG_STATUTORY_CHANGE 
	END AS CHAR(1)) AS IND_10,

	CASE
		WHEN NULLIF(FLG_STATUTORY_CHANGE, '') IS NULL THEN 'E0001' ELSE ''
	END AS ERROR_MSG_IND_10


";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
