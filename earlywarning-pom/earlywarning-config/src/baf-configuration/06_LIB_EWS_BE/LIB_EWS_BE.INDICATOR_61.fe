				OUTFLOWS_AMOUNT_M5 IS NULL OR				 
				OUTFLOWS_AMOUNT_M6 IS NULL OR
				OUTFLOWS_AMOUNT_M7 IS NULL OR
				OUTFLOWS_AMOUNT_M8 IS NULL OR				 
				OUTFLOWS_AMOUNT_M9 IS NULL OR
				OUTFLOWS_AMOUNT_M10 IS NULL OR
				OUTFLOWS_AMOUNT_M11 IS NULL
				THEN MISSING_VALUE_IND_61
	ELSE ((OUTFLOWS_AMOUNT_M0+OUTFLOWS_AMOUNT_M1+OUTFLOWS_AMOUNT_M2+OUTFLOWS_AMOUNT_M3+OUTFLOWS_AMOUNT_M4+
			OUTFLOWS_AMOUNT_M5+OUTFLOWS_AMOUNT_M6+OUTFLOWS_AMOUNT_M7+OUTFLOWS_AMOUNT_M8+OUTFLOWS_AMOUNT_M9+OUTFLOWS_AMOUNT_M10+
			OUTFLOWS_AMOUNT_M11)/12) 
END AS IND_61,

(CASE
   WHEN 
