VAR formulaName = "[LIB_EWS_BE.INDICATOR_61.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
CASE 
	WHEN COUNTRY = 'BIB' AND REGULAMENTARY_SEGMENT = 'Corporate' THEN 178372.33
	WHEN COUNTRY = 'BIB' AND REGULAMENTARY_SEGMENT = 'Retail' THEN 6343.7617
	ELSE NULL
END AS MISSING_VALUE_IND_61,

CASE 
	WHEN 
				OUTFLOWS_AMOUNT_M0  IS NULL OR
				OUTFLOWS_AMOUNT_M1 IS NULL OR
				OUTFLOWS_AMOUNT_M2 IS NULL OR
				OUTFLOWS_AMOUNT_M3 IS NULL OR
