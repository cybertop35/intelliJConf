	  END AS CHAR(1)) AS IND_4,

CASE
 	WHEN NULLIF(FLG_BANK_CHEQUES,'') IS NULL THEN 'E0001' ELSE ''
END AS ERROR_MSG_IND_4


";


DEBUGMESSAGE(formulaName, sql);
