VAR formulaName = "[LIB_EWS_BE.INDICATOR_180.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "CASE 
	WHEN OVERDRAFT_LIMIT_M0 = 0 AND INFLOWS_ACCOUNT_M0 > 0 THEN NULL
	WHEN OVERDRAFT_LIMIT_M0 = 0 AND INFLOWS_ACCOUNT_M0 < 0 THEN NULL
	WHEN OVERDRAFT_LIMIT_M0 = 0 AND INFLOWS_ACCOUNT_M0 = 0 THEN NULL
	ELSE (INFLOWS_ACCOUNT_M0/OVERDRAFT_LIMIT_M0)
END AS IND_180";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
