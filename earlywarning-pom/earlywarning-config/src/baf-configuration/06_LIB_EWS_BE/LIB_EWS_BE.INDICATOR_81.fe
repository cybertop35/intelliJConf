VAR formulaName = "[LIB_EWS_BE.INDICATOR_81.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "CASE
WHEN (((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2)) = 0 AND SALES_Y0 >0 THEN NULL
WHEN (((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2)) = 0 AND SALES_Y0 <0 THEN NULL
WHEN (((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2)) = 0 AND SALES_Y0 =0 THEN NULL

WHEN (((ACCOUNT_RECEIVABLES_Y1+ACCOUNT_RECEIVABLES_Y2)/2)) = 0 AND SALES_Y1 >0 THEN NULL
WHEN (((ACCOUNT_RECEIVABLES_Y1+ACCOUNT_RECEIVABLES_Y2)/2)) = 0 AND SALES_Y1 <0 THEN NULL
WHEN (((ACCOUNT_RECEIVABLES_Y1+ACCOUNT_RECEIVABLES_Y2)/2)) = 0 AND SALES_Y1 =0 THEN NULL

WHEN ((SALES_Y1/((ACCOUNT_RECEIVABLES_Y1+ACCOUNT_RECEIVABLES_Y2)/2)) = 0 AND (SALES_Y0/((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2)) >0) THEN NULL
WHEN ((SALES_Y1/((ACCOUNT_RECEIVABLES_Y1+ACCOUNT_RECEIVABLES_Y2)/2)) = 0 AND (SALES_Y0/((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2)) <0) THEN NULL
WHEN ((SALES_Y1/((ACCOUNT_RECEIVABLES_Y1+ACCOUNT_RECEIVABLES_Y2)/2)) = 0 AND (SALES_Y0/((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2)) =0) THEN NULL
 
ELSE ((SALES_Y0/((ACCOUNT_RECEIVABLES_Y0+ACCOUNT_RECEIVABLES_Y1)/2)) / (SALES_Y1/((ACCOUNT_RECEIVABLES_Y1+ACCOUNT_RECEIVABLES_Y2)/2)))-1 
END AS IND_81";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
