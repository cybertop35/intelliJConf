VAR sql = "";

sql = "

CASE 
	WHEN (EBITDA_Y0 > 0 AND TOTAL_DEBT_Y0 >0) THEN (TOTAL_DEBT_Y0/EBITDA_Y0)
	WHEN (EBITDA_Y0 > 0 AND TOTAL_DEBT_Y0 =0) THEN (TOTAL_DEBT_Y0/EBITDA_Y0)
	WHEN (EBITDA_Y0 > 0 AND TOTAL_DEBT_Y0 <0) THEN (TOTAL_DEBT_Y0/EBITDA_Y0)
	WHEN (EBITDA_Y0 > 0 AND TOTAL_DEBT_Y0 IS NULL) THEN 0

	WHEN (EBITDA_Y0 = 0 AND TOTAL_DEBT_Y0 >0) THEN 0         
	WHEN (EBITDA_Y0 = 0 AND TOTAL_DEBT_Y0 <0) THEN 0
	WHEN (EBITDA_Y0 = 0 AND TOTAL_DEBT_Y0 =0) THEN 0
