WHEN (EBITDA_Y0 IS NULL AND TOTAL_DEBT_Y0 =0) THEN 'ERROR; ' else ''
END || CASE
WHEN (EBITDA_Y0 IS NULL AND TOTAL_DEBT_Y0 IS NULL) THEN 'ERROR; ' else ''
END
) AS ERROR_MSG_IND_64

