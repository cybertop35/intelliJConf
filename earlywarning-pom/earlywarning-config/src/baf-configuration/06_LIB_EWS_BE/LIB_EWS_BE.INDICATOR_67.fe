VAR formulaName = "[LIB_EWS_BE.INDICATOR_67.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "CASE 
	WHEN EBITDA_Y0 = 0 AND MAX_TOT_DEBT_6M> 0 THEN NULL
	WHEN EBITDA_Y0 = 0 AND MAX_TOT_DEBT_6M< 0 THEN NULL
	WHEN EBITDA_Y0 = 0 AND MAX_TOT_DEBT_6M= 0 THEN NULL
	ELSE (MAX_TOT_DEBT_6M/EBITDA_Y0)
END AS IND_67";


DEBUGMESSAGE(formulaName, sql);
