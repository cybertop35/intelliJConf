	WHEN (ST_LIABILITIES_Y0 IS NULL) THEN NULL
	WHEN (LT_LIABILITIES = 0) THEN NULL
	WHEN (LT_LIABILITIES IS NULL) THEN NULL
END AS NUM_IND_104, 

CASE
	WHEN (DEN_IND_104 <> 0 AND DEN_IND_104 IS NOT NULL AND NUM_IND_104 IS NOT NULL) THEN (NUM_IND_104 / DEN_IND_104)
	WHEN (NUM_IND_104 IS NULL) THEN MISSING_VALUE_IND_104
	WHEN (DEN_IND_104 = 0) THEN MISSING_VALUE_IND_104
	WHEN (DEN_IND_104 IS NULL) THEN MISSING_VALUE_IND_104 
END AS IND_104,

CASE 
	WHEN (DEN_IND_104 <> 0 AND DEN_IND_104 IS NOT NULL AND NUM_IND_104 IS NOT NULL) THEN ''
	WHEN (NUM_IND_104 IS NULL) THEN 'E0001 '
	WHEN (DEN_IND_104 = 0) THEN 'E0001 '
	WHEN (DEN_IND_104 IS NULL) THEN 'E0001 ' 
END AS ERROR_MSG_IND_104


";


