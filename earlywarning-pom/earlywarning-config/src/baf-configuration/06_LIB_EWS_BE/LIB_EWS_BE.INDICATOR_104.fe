VAR formulaName = "[LIB_EWS_BE.INDICATOR_104.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


0 AS MISSING_VALUE_IND_104,

CASE
	WHEN (LT_ASSETS <> 0 AND LT_ASSETS IS NOT NULL AND ST_ASSETS_Y0 IS NOT NULL) THEN (ST_ASSETS_Y0 / (LT_ASSETS))
