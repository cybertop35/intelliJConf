		(NVL2(IMP_USC_CORR_M0,1,0) + NVL2(IMP_USC_CORR_M1,1,0) + NVL2(IMP_USC_CORR_M2,1,0)) AS DECIMAL(30,6))
	END AS DECIMAL(18,6)) AS IND_226,
		
	CASE 
