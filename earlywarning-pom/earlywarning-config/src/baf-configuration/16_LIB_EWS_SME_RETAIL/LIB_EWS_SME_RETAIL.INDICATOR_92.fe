CASE 
	WHEN IMP_UTIL_REV_M0 IS NOT NULL OR
		 IMP_UTIL_REV_M1 IS NOT NULL OR
		 IMP_UTIL_REV_M2 IS NOT NULL THEN 
		 ((CAST((zeroifnull(IMP_UTIL_REV_M0) + zeroifnull(IMP_UTIL_REV_M1) + zeroifnull(IMP_UTIL_REV_M2)) AS DECIMAL(30,4)))/
