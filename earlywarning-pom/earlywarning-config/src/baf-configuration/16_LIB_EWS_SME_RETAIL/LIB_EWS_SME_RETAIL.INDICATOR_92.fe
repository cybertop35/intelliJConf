	WHEN IMP_ACC_REV_M0 IS NOT NULL OR
		 IMP_ACC_REV_M1 IS NOT NULL OR
		 IMP_ACC_REV_M2 IS NOT NULL THEN 
		 ((CAST((zeroifnull(IMP_ACC_REV_M0) + zeroifnull(IMP_ACC_REV_M1) + zeroifnull(IMP_ACC_REV_M2)) AS DECIMAL(30,4)))/
		(NVL2(IMP_ACC_REV_M0,1,0)+NVL2(IMP_ACC_REV_M1,1,0)+NVL2(IMP_ACC_REV_M2,1,0)))
		 ELSE NULL
END AS DEN_IND_92,

