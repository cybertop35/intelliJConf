		WHEN NUM_IND_92 > 0 AND DEN_IND_92 = 0            THEN 1000000
		ELSE (NUM_IND_92/DEN_IND_92) 
	END AS DECIMAL(20,4)) AS IND_92 ,
				
	CASE	
		WHEN NUM_IND_92 IS NULL AND DEN_IND_92 IS NULL    THEN 'CRSYS000_1'
