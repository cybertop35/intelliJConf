	CASE
		WHEN NUM_IND_92 IS NULL AND DEN_IND_92 IS NULL    THEN NULL
		WHEN NUM_IND_92 = 0 AND DEN_IND_92 = 0            THEN -9999999
		WHEN NUM_IND_92 IS NULL AND DEN_IND_92 > 0        THEN 9999999
