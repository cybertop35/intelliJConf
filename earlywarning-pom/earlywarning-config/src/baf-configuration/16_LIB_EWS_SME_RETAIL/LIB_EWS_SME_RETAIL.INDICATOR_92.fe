		WHEN NUM_IND_92 IS NULL AND DEN_IND_92 = 0        THEN 'CRSYS003_2'
		WHEN NUM_IND_92 > 0 AND DEN_IND_92 IS NULL        THEN 'CRSYS003_1'
		WHEN NUM_IND_92 = 0 AND DEN_IND_92 IS NULL        THEN 'CRSYS003_1'
		WHEN NUM_IND_92 < 0 AND DEN_IND_92 > 0            THEN 'CRSYS004_1'
		WHEN NUM_IND_92 < 0 AND DEN_IND_92 = 0            THEN 'CRSYS004_1'
		WHEN NUM_IND_92 < 0 AND DEN_IND_92 < 0            THEN 'CRSYS004_1; CRZER004_2'
		WHEN NUM_IND_92 > 0 AND DEN_IND_92 < 0            THEN 'CRSYS004_2'
		WHEN NUM_IND_92 = 0 AND DEN_IND_92 < 0            THEN 'CRSYS004_2'
		ELSE ''
	END AS ERROR_MSG_IND_92
";

