VAR formulaName = "[LIB_EWS_RETAIL.INDICATOR_70.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

CAST(
	CASE 
		WHEN IND_71 IS NULL AND
			 IND_72 IS NULL THEN '0'
		ELSE '1'
	END AS CHAR(1)) AS IND_70,

	CAST(NULL AS VARCHAR(20)) AS ERROR_MSG_IND_70

";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;

