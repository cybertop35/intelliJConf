VAR formulaName = "[LIB_EWS_IT.INDICATOR_260.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
CASE 
	WHEN IMP_UTIL_PROD_SCAD_NOCOL_M0 IS NOT NULL OR
		 IMP_UTIL_PROD_SCAD_NOCOL_M1 IS NOT NULL OR
		 IMP_UTIL_PROD_SCAD_NOCOL_M2 IS NOT NULL THEN 
		((CAST((zeroifnull(IMP_UTIL_PROD_SCAD_NOCOL_M0) + zeroifnull(IMP_UTIL_PROD_SCAD_NOCOL_M1) + zeroifnull(IMP_UTIL_PROD_SCAD_NOCOL_M2)) AS DECIMAL(30,12)))/
		(NVL2(IMP_UTIL_PROD_SCAD_NOCOL_M0,1,0)+NVL2(IMP_UTIL_PROD_SCAD_NOCOL_M1,1,0)+NVL2(IMP_UTIL_PROD_SCAD_NOCOL_M2,1,0)))
		 ELSE NULL
END AS NUM_IND_260,


CASE 
	WHEN IMP_ACC_PROD_SCAD_NOCOL_M0 IS NOT NULL OR
		 IMP_ACC_PROD_SCAD_NOCOL_M1 IS NOT NULL OR
		 IMP_ACC_PROD_SCAD_NOCOL_M2 IS NOT NULL THEN 
		 ((CAST((zeroifnull(IMP_ACC_PROD_SCAD_NOCOL_M0) + zeroifnull(IMP_ACC_PROD_SCAD_NOCOL_M1) + zeroifnull(IMP_ACC_PROD_SCAD_NOCOL_M2)) AS DECIMAL(30,12)))/
		(NVL2(IMP_ACC_PROD_SCAD_NOCOL_M0,1,0)+NVL2(IMP_ACC_PROD_SCAD_NOCOL_M1,1,0)+NVL2(IMP_ACC_PROD_SCAD_NOCOL_M2,1,0)))
		 ELSE NULL
END AS DEN_IND_260,

CAST(	
	CASE
		WHEN NUM_IND_260 IS NULL AND DEN_IND_260 IS NULL    THEN NULL
		WHEN NUM_IND_260 = 0 AND DEN_IND_260 = 0            THEN -9999999
		WHEN NUM_IND_260 IS NULL AND DEN_IND_260 > 0        THEN 9999999
		WHEN NUM_IND_260 IS NULL AND DEN_IND_260 = 0        THEN 9999999
		WHEN NUM_IND_260 > 0 AND DEN_IND_260 IS NULL        THEN -1000000
		WHEN NUM_IND_260 = 0 AND DEN_IND_260 IS NULL        THEN 0
		WHEN NUM_IND_260 < 0 AND DEN_IND_260 > 0            THEN NULL
		WHEN NUM_IND_260 < 0 AND DEN_IND_260 = 0            THEN NULL
		WHEN NUM_IND_260 < 0 AND DEN_IND_260 < 0            THEN NULL
		WHEN NUM_IND_260 > 0 AND DEN_IND_260 < 0            THEN NULL
		WHEN NUM_IND_260 = 0 AND DEN_IND_260 < 0            THEN NULL
		WHEN NUM_IND_260 > 0 AND DEN_IND_260 = 0            THEN 1000000
		ELSE (NUM_IND_260/DEN_IND_260) 
	END AS DECIMAL(18,6)) AS IND_260 ,
				
	CASE	
		WHEN NUM_IND_260 IS NULL AND DEN_IND_260 IS NULL    THEN 'CRSYS000_1'
		WHEN NUM_IND_260 > 0 AND DEN_IND_260 = 0            THEN 'CRSYS002_1'
		WHEN NUM_IND_260 = 0 AND DEN_IND_260 = 0            THEN 'CRSYS002_2'
		WHEN NUM_IND_260 IS NULL AND DEN_IND_260 > 0        THEN 'CRSYS003_2'
		WHEN NUM_IND_260 IS NULL AND DEN_IND_260 = 0        THEN 'CRSYS003_2'
		WHEN NUM_IND_260 > 0 AND DEN_IND_260 IS NULL        THEN 'CRSYS003_1'
		WHEN NUM_IND_260 = 0 AND DEN_IND_260 IS NULL        THEN 'CRSYS003_1'
		WHEN NUM_IND_260 < 0 AND DEN_IND_260 > 0            THEN 'CRSYS004_1'
		WHEN NUM_IND_260 < 0 AND DEN_IND_260 = 0            THEN 'CRSYS004_1'
		WHEN NUM_IND_260 < 0 AND DEN_IND_260 < 0            THEN 'CRSYS004_1; CRZER004_2'
		WHEN NUM_IND_260 > 0 AND DEN_IND_260 < 0            THEN 'CRSYS004_2'
		WHEN NUM_IND_260 = 0 AND DEN_IND_260 < 0            THEN 'CRSYS004_2'
		ELSE ''
	END AS ERROR_MSG_IND_260

";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;