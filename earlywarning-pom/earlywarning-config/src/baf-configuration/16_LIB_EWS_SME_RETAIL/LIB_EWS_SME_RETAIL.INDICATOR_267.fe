VAR formulaName = "[LIB_EWS_IT.INDICATOR_267.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

IMP_ANTEXP_30D_NOCOL_M0 AS NUM_IND_267,

IMP_ACC_S_T_C_M0 AS DEN_IND_267,	


CAST(	
	CASE
		WHEN NUM_IND_267 IS NULL AND DEN_IND_267 IS NULL    THEN NULL
		WHEN NUM_IND_267 = 0 AND DEN_IND_267 = 0            THEN 0
		WHEN NUM_IND_267 IS NULL AND DEN_IND_267 > 0        THEN NULL
		WHEN NUM_IND_267 IS NULL AND DEN_IND_267 = 0        THEN NULL
		WHEN NUM_IND_267 > 0 AND DEN_IND_267 IS NULL        THEN 9999999
		WHEN NUM_IND_267 = 0 AND DEN_IND_267 IS NULL        THEN 0
		WHEN NUM_IND_267 < 0 AND DEN_IND_267 > 0            THEN NULL
		WHEN NUM_IND_267 < 0 AND DEN_IND_267 = 0            THEN NULL
		WHEN NUM_IND_267 < 0 AND DEN_IND_267 < 0            THEN NULL
		WHEN NUM_IND_267 > 0 AND DEN_IND_267 < 0            THEN NULL
		WHEN NUM_IND_267 = 0 AND DEN_IND_267 < 0            THEN NULL
		WHEN NUM_IND_267 > 0 AND DEN_IND_267 = 0            THEN 1000000
		ELSE (NUM_IND_267/DEN_IND_267) 
	END AS DECIMAL(18,6)) AS IND_267 ,
				
	CASE	
		WHEN NUM_IND_267 IS NULL AND DEN_IND_267 IS NULL    THEN 'ANTEXP000_1'
		WHEN NUM_IND_267 > 0 AND DEN_IND_267 = 0            THEN 'ANTEXP002_1'
		WHEN NUM_IND_267 = 0 AND DEN_IND_267 = 0            THEN 'ANTEXP002_2'
		WHEN NUM_IND_267 IS NULL AND DEN_IND_267 > 0        THEN 'ANTEXP000_2'
		WHEN NUM_IND_267 IS NULL AND DEN_IND_267 = 0        THEN 'ANTEXP000_2'
		WHEN NUM_IND_267 > 0 AND DEN_IND_267 IS NULL        THEN 'ANTEXP001_2'
		WHEN NUM_IND_267 = 0 AND DEN_IND_267 IS NULL        THEN 'ANTEXP001_1'
		WHEN NUM_IND_267 < 0 AND DEN_IND_267 > 0            THEN 'ANTEXP004_1'
		WHEN NUM_IND_267 < 0 AND DEN_IND_267 = 0            THEN 'ANTEXP004_1'
		WHEN NUM_IND_267 < 0 AND DEN_IND_267 < 0            THEN 'ANTEXP004_1; ANTEXP004_2'
		WHEN NUM_IND_267 > 0 AND DEN_IND_267 < 0            THEN 'ANTEXP004_2'
		WHEN NUM_IND_267 = 0 AND DEN_IND_267 < 0            THEN 'ANTEXP004_2'
		ELSE ''
	END AS ERROR_MSG_IND_267


";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;


