VAR formulaName = "[LIB_EWS_IT.INDICATOR_42.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(
	CASE 
		WHEN RATING IS NULL THEN NULL
		ELSE RATING
	END AS CHAR(4)) AS IND_42,

	CASE 
		WHEN RATING IS NULL THEN 'E0001'
		ELSE ''
	END AS ERROR_MSG_IND_42 

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
