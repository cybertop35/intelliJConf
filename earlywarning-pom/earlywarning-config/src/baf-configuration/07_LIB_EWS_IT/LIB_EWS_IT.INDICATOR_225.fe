VAR formulaName = "[LIB_EWS_IT.INDICATOR_225.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(
	CASE 
		WHEN(AMT_MAX_OVERDRAFT_3M IS NULL )  THEN 9999999
		ELSE GREATEST(AMT_MAX_OVERDRAFT_3M)
	END AS DECIMAL(18,6)) AS IND_225,

	CASE 
		WHEN(AMT_MAX_OVERDRAFT_3M IS NULL) THEN 'XRA001' 
		ELSE ''
	END AS ERROR_MSG_IND_225 

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
