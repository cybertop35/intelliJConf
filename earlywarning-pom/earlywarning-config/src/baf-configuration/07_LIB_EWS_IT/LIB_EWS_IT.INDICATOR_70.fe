VAR formulaName = "[LIB_EWS_IT.INDICATOR_70.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(
	CASE 
		WHEN OVERDUE_DD_FACT IS NULL THEN NULL
		WHEN OVERDUE_DD_FACT < 0 THEN 9999999
		ELSE OVERDUE_DD_FACT
	END AS DECIMAL(10,0)) AS IND_70,

	CASE 
		WHEN(OVERDUE_DD_FACT IS NULL OR OVERDUE_DD_FACT < 0 ) THEN 'FACT0001' 
		ELSE ''
	END AS ERROR_MSG_IND_70 

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
