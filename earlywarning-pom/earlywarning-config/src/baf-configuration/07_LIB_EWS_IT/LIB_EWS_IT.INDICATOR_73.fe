VAR formulaName = "[LIB_EWS_IT.INDICATOR_73.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(
	CASE
		WHEN EDF_MIR IS NOT NULL THEN EDF_MIR
		WHEN EDF_CREDIT_EDGE IS NOT NULL THEN EDF_CREDIT_EDGE
		ELSE NULL
	END AS CHAR(10)) AS IND_73,
 
	CASE 
		WHEN EDF_CREDIT_EDGE IS NULL THEN 'MKT000_1'
		WHEN EDF_MIR IS NULL THEN 'MKT000_1'
		ELSE ''
	END AS ERROR_MSG_IND_73 


";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
