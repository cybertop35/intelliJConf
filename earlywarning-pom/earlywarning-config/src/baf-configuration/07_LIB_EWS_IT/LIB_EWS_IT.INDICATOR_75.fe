VAR formulaName = "[LIB_EWS_IT.INDICATOR_75.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(
	CASE
		WHEN CDS_IMPLIED_RATING IS NOT NULL THEN CDS_IMPLIED_RATING
		WHEN CE_CDS_IMPLIED_RATING IS NOT NULL THEN CE_CDS_IMPLIED_RATING 
		ELSE NULL
	END AS CHAR(10)) AS IND_75,

	CASE 
		WHEN(CDS_IMPLIED_RATING IS NULL) THEN 'MARK001' 
		ELSE ''
	END AS ERROR_MSG_IND_75 

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
