VAR formulaName = "[LIB_EWS_IT.IND_12_FL_OVERRIDE.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
CAST(
		CASE 
			WHEN BR_12_OVERRIDE  = '0' AND IND_12 >= VAL_BR_12_OVERRIDE THEN '1'
			ELSE '0' 
		END AS CHAR(1)) AS IND_12_FL_OVERRIDE,
		
		(CASE
			WHEN BR_12_OVERRIDE  = '0' AND IND_12 >= VAL_BR_12_OVERRIDE THEN ''
			ELSE ''
		END ) AS ERROR_MSG_IND_12_FL_OVERRIDE
";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;