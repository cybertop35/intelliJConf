VAR formulaName = "[LIB_EWS_IT.INDICATOR_60.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(
	CASE 

		WHEN OVERDRAFT_DD_3M IS NULL THEN -8888888
		WHEN OVERDRAFT_DD_3M < 0 THEN 9999999
		ELSE OVERDRAFT_DD_3M
	END AS DECIMAL(10,0)) AS IND_60,

	CASE 
		WHEN OVERDRAFT_DD_3M IS NULL THEN 'XRA000_1'
		WHEN OVERDRAFT_DD_3M < 0 THEN 'XRA004_1'
		ELSE ''
	END AS ERROR_MSG_IND_60 

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
