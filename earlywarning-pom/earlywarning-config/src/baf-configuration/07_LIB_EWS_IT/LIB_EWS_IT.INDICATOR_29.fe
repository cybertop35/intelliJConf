VAR formulaName = "[LIB_EWS_IT.INDICATOR_29.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

CAST(
	CASE 
		WHEN COVE_FLG_ROTTURA = 'S' THEN CURRENT_DATE - COVE_DATA_INIZIO_ROTTURA
		ELSE null
	END AS DECIMAL(10,0)) AS IND_29,
	
NULL AS ERROR_MSG_IND_29

";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;


