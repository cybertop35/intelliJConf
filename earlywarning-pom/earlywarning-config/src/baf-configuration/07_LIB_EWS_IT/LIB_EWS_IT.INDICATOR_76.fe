VAR formulaName = "[LIB_EWS_IT.INDICATOR_76.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

CAST(
	CASE 
		WHEN CDS_SPREAD_5Y IS NULL THEN NULL
		WHEN CDS_SPREAD_5Y < 0 THEN 9999999
		ELSE CDS_SPREAD_5Y
	END AS DECIMAL(30,6)) AS IND_76,

	CASE 
		WHEN(CDS_SPREAD_5Y IS NULL) THEN 'MARK001' 
		ELSE ''
	END AS ERROR_MSG_IND_76 

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
