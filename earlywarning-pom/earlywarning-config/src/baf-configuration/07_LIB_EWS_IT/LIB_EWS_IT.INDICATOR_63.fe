VAR formulaName = "[LIB_EWS_IT.INDICATOR_63.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "


CAST(
	CASE 
		WHEN OVERDUE_DD_FAT IS NULL THEN NULL
		WHEN OVERDUE_DD_FAT <= 60 THEN -9999999
		ELSE OVERDUE_DD_FAT
	END AS DECIMAL(10,0)) AS IND_63,

	CASE 
		WHEN OVERDUE_DD_FAT IS NULL THEN 'NPAF000_1' 
		WHEN OVERDUE_DD_FAT <= 60 THEN 'NPAF004_3' 
		ELSE ''
	END AS ERROR_MSG_IND_63 

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
