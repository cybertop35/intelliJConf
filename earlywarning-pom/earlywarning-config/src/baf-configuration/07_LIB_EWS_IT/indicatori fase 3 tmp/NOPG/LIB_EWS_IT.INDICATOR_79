VAR formulaName = "[LIB_EWS_IT.INDICATOR_79.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "
CAST(
	CASE
		WHEN BANKRUP_APPL_S10 IS NOT NULL THEN 'S10'
		WHEN BANKRUP_APPL_S20 IS NOT NULL THEN 'S20'
		WHEN BANKRUP_APPL_S30 IS NOT NULL THEN 'S30'
		WHEN BANKRUP_APPL_S40 IS NOT NULL THEN 'S40'
		ELSE NULL
	END AS CHAR(10)) AS IND_79,
	
	'' AS ERROR_MSG_IND_79
";

DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;