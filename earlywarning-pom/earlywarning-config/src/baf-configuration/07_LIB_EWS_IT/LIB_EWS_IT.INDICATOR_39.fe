VAR formulaName = "[LIB_EWS_IT.INDICATOR_39.fe] ";
DEBUGMESSAGE(formulaName, "START");

VAR sql = "";

sql = "

(TOT_EQUITY_T1 - GOODWILL_T1 - OTH_INTANGIBLES_T1) as NUM_IND_39,

(TOT_ASSETS_T1 - GOODWILL_T1 - OTH_INTANGIBLES_T1) AS DEN_IND_39,

CAST(
	CASE
		WHEN ELIGIBLE_FIN_STAT_COND_1_BF = 0 OR ELIGIBLE_FIN_STAT_COND_2_BF= 0 OR ELIGIBLE_FIN_STAT_COND_3_BF = 0 OR ELIGIBLE_FIN_STAT_COND_4_BF =0 THEN NULL
		WHEN ELIGIBLE_FIN_STAT_COND_1_BF IS NULL OR ELIGIBLE_FIN_STAT_COND_2_BF IS NULL OR ELIGIBLE_FIN_STAT_COND_3_BF IS NULL OR ELIGIBLE_FIN_STAT_COND_4_BF IS NULL THEN NULL
		WHEN NUM_IND_39 IS NULL OR DEN_IND_39 IS NULL THEN NULL
		WHEN DEN_IND_39 <= 0 THEN NULL 
		WHEN DEN_IND_39 > 0 THEN (NUM_IND_39/DEN_IND_39)
	END AS DECIMAL(30,12)) AS IND_39 ,

	CASE
		WHEN NUM_IND_39 IS NULL AND DEN_IND_39 IS NULL THEN 'LEV000_1'
		WHEN DEN_IND_39 = 0 THEN 'LEV004_1'
		WHEN DEN_IND_39 < 0 THEN 'LEV004_2'	
		ELSE ''
	END AS ERROR_MSG_IND_39 

";


DEBUGMESSAGE(formulaName, sql);
DEBUGMESSAGE(formulaName, "END");

return sql;
