VAR puntoDiprelievo=Param1;
VAR typeCurve=Param2;
VAR hour=Param3;
VAR tsType=LibDemandPlanning.getClimaticZoneTS(puntoDiprelievo,typeCurve);
VAR resultTs;
IF(GETUDM(tsType)<>NULL){
	resultTs=TS("DAILY",GETUDM(tsType));
}ELSE{
	resultTs=TS("DAILY","");
}
FOREACHDATE(tsType AS dateType){
	IF(GETHOUR(dateType)==hour){
		SETVALUE(resultTs,dateType,VALUE(tsType,dateType));
	}

}
RETURN resultTs;